{
  "expo": {
    "name": "DailyWallet",
    "slug": "DailyWallet",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon-3.png",
    "scheme": "dailywallet",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/icon-3.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.shahin.dailywallet",
      "versionCode": 3,
      "adaptiveIcon": {
        "backgroundColor": "#ffffff00",
        "foregroundImage": "./assets/images/icon-3.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon-3.png"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "f469ec98-3407-4668-80f4-2ca5d83d8208"
      }
    }
  }
}
